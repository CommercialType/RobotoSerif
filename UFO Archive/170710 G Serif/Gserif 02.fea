languagesystem DFLT dflt;
languagesystem latn dflt;

@CGS_SERIF_OFF = [C G];
@CGS_SERIF_ON = [C.srf G.srf];

@EF_ALT_OFF = [E F];
@EF_ALT_ON = [E.alt F.alt];


feature ss01 {
	sub @CGS_SERIF_OFF by @CGS_SERIF_ON;
} ss01;

feature ss02 {
	sub @EF_ALT_OFF by @EF_ALT_ON;
} ss02;

feature ss03 {
	sub g by g.alt;
} ss03;

feature ss04 {
	sub f by f.alt;
} ss04;



