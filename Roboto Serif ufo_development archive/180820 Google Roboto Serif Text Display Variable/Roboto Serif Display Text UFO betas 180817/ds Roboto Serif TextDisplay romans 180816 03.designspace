                <dimension name="weight" xvalue="745" />
                <dimension name="optical" xvalue="0" />
            </location>
            <kerning />
            <info />
        </instance>
        <instance familyname="Roboto Serif Text" filename="instances/RobotoSerifText-Black.ufo" postscriptfontname="RobotoSerifText-Black" stylename="Text Black">
            <location>
                <dimension name="weight" xvalue="825" />
                <dimension name="optical" xvalue="0" />
            </location>
            <kerning />
            <info />
        </instance>
    </instances>
</designspace>
